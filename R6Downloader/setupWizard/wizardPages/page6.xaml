<UserControl x:Class="R6Downloader.setupWizard.wizardPages.page6"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:R6Downloader.setupWizard.wizardPages"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Background="Transparent">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Step 5:" Foreground="White" HorizontalAlignment="Center" FontSize="20"/>
            <TextBlock Text="Setup Steamguard:" Foreground="White"
                       HorizontalAlignment="Center" FontSize="14"/>
            <Button Style="{StaticResource UniversalMaterialButton}"
                    Height="30"
                    Margin="5"
                    Background="#38364d"
                    Content="Setup here"
                    Click="ButtonBase_OnClick"/>
        </StackPanel>
        
    </Grid>
</UserControl>
