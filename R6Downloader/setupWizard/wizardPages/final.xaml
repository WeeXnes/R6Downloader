<UserControl x:Class="R6Downloader.setupWizard.wizardPages.final"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:R6Downloader.setupWizard.wizardPages"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Background="Transparent">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Text="You're all set" Foreground="White" HorizontalAlignment="Center" FontSize="20"/>
            <TextBlock Text="the setup is now complete!" Foreground="White" HorizontalAlignment="Center" FontSize="12"/>
            <Button Margin="0,20,0,0"
                    Height="30"
                    Width="120"
                    Style="{StaticResource UniversalMaterialButton}"
                    Content="Finish"
                    Background="#38364d"
                    Click="ButtonBase_OnClick"/>
        </StackPanel>
    </Grid>
</UserControl>
